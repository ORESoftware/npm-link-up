{
  "$schema": "https://raw.githubusercontent.com/oresoftware/npm-link-up/master/assets/nlu.schema.json",
  "npm-link-up": true,
  "linkToItself": false,
  "comments": [
    "your comments go here"
  ],
  "searchRoots": [
    "$nlu_primary_search_root"
  ],
  "ignore": [
    "/node_modules/",
    "/.git/"
  ],
  "list": [
    "@oresoftware/envstr",
    "@oresoftware/shell",
    "prepend-transform",
    "residence",
    "json-stdio",
    "clean-trace"
  ],
  "localSettings": {
    "bar": "23",
    "foo": "88"
  }
}
